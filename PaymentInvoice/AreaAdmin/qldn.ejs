<style>
  table {
    border-collapse: collapse;
  }

  tr:nth-child(even) {
    background-color: #f2f2f2;
  }

  th {
    padding-top: 12px;
    padding-bottom: 12px;
    text-align: center;
    background-color: gray;
    color: white;
  }
</style>
<div id="custom-search-input">
  <div class="input-group col-md-12">
    <form method="GET" action="/admin-qldn">
      <input type="search" name="search" id="input" class="search-query form-control" value="" title=""
        placeholder="Từ khóa tìm kiếm" style="width: 50%;margin-top: 25px;margin-left: 210px;margin-bottom:10px;">
      <span class="action-search">
        <button class="btn-search" type="submit">
          <i class="fa fa-search" style="font-size:20px!important; position: absolute;right: 210px;top: 25px;"></i>
        </button>
      </span>
    </form>
    <div class="scrollable">
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Tên DN</th>
            <th>Mã DN</th>
            <th>Số DT</th>
            <th>Email</th>
            <th>Loại DN</th>
          </tr>
        </thead>
        <tbody>
          <%for(let i = 0; i < data.Items.length; i++){%>
          <%if(data.Items[i].loaiTK == 2){%>
          <tr>
            <td><%=data.Items[i].doanhnghiep.tenDN%></td>
            <td><%=data.Items[i].maDN%></td>
            <td><%=data.Items[i].doanhnghiep.sodt%></td>
            <td><%=data.Items[i].email%></td>
            <%if(data.Items[i].doanhnghiep.loaiDN == 1){%>
            <td>Điện</td>
            <%}%>
                      <%if(data.Items[i].doanhnghiep.loaiDN == 2){%>
            <td>Nước</td>
            <%}%>
                      <%if(data.Items[i].doanhnghiep.loaiDN == 3){%>
            <td>Internet</td>
            <%}%>
                     
                    </tr>
                    <%}%>
            <%}%>
              </tbody>
            </table>
          </div>
          </div>
      </div>