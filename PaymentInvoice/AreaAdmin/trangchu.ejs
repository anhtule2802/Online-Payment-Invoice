<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Trang Chủ Công Nghệ Mới</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <link rel="stylesheet" type="text/css" href="css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="css/mmenu.css">
    <link href="css/mmenu.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="css/swiper.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
</head>

<body>
    <!-- header -->
    <% include header %>

    <!-- contain -->
    <section class="home-payment">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12 payment-title">
                    <% include headerBody %>
                </div>
                <div class="col-md-12 col-sm-12 col-xs-12 payment-content">
                    <div class="col-md-9 col-sm-12 col-xs-12 content-center">
                        <div class="content-center-div">
                                <%-body%>
                        </div>
                    </div>
                    <div class="col-md-3 hidden-sm hidden-xs content-right">
                        <div class="content-right-div">
                            <% include content-body-right%>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <% include footer %>

    <!-- jQuery -->
    <!-- <script src="js/jquery.js"></script> -->
    <script src="js/mmenu.js"></script>
    <!-- Bootstrap JavaScript -->
    <!-- <script src="js/bootstrap.min.js"></script> -->
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="js/jquery-3.3.1.slim.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap-3.4.0.min.js"></script>
    <script src="js/sweetalert.min.js"></script>
    <script>
        Mmenu.configs.offCanvas.page.selector = "#my-page";

        document.addEventListener(
            "DOMContentLoaded", () => {
                new Mmenu("#my-menu", {
                    dividers: {
                        addTo: "#my-contacts",
                        fixed: true
                    },
                });
            }
        );
    </script>
    <script>
        $(".title-center").hover(function () {
            $(this).stop(true, false).animate({ padding: "10px" });
        }, function () {
            $(this).stop(true, false).animate({ padding: "0" });
        });
    </script>
